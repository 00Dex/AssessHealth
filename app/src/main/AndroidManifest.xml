<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.trust.assesshealth">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/check"
        android:label="Check Health"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activity.SelectStudentActivity" />
        <activity android:name=".Activity.AlertActivity" />
        <activity android:name=".Activity.AddActivity" />
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.AssessStudentActivity"/>
        <activity android:name=".Activity.RegistrationActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>